#!/usr/bin/env bash
echo "-----> Nothing to compile"
echo "-----> pwd: $(pwd)"
echo "-----> Downloading ffmpeg"
curl -sSL -o ffmpeg.xz https://johnvansickle.com/ffmpeg/releases/ffmpeg-release-amd64-static.tar.xz
echo "-----> Downloaded ffmpeg"
ls -l -a
echo "-----> Decompressing ffmpeg"
xz -d -c ffmpeg.xz >ffmpeg
echo "-----> Decompressed ffmpeg"
ls -l -a
echo "-----> Listing bin:"
ls -l -a bin/
echo "-----> Go version:"
go version
echo "-----> Args:" $*
echo "-----> Env:"
env
echo "-----> Listing whole filesystem"
find /
